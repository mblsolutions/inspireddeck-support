{
  "name": "mblsolutions/inspireddeck-support",
  "description": "Support for Official Inspired Deck applications",
  "keywords": ["inspired", "deck"],
  "license": "MIT",
  "authors": [
    {
      "name": "MBL Solutions Ltd.",
      "email": "support@mblsolutions.co.uk"
    }
  ],
  "require": {
    "php": "^8.2",
    "ext-json": "*",
    "erusev/parsedown-extra": "^0.8.1",
    "illuminate/support": "^10.0",
    "illuminate/http": "^10.0",
    "mblsolutions/inspireddeck-php-laravel": "^2.2"
  },
  "require-dev": {
    "mockery/mockery": "^1.5",
    "orchestra/testbench": "^8.0",
    "phpunit/phpunit": "^10.5"
  },
  "autoload": {
    "psr-4": {
      "MBLSolutions\\InspiredDeckSupport\\": "src/"
    },
    "files": [
      "src/Support/helpers.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "MBLSolutions\\InspiredDeckSupport\\Tests\\": "tests"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "laravel": {
      "providers": [
        "MBLSolutions\\InspiredDeckSupport\\InspiredDeckServiceProvider"
      ]
    }
  }
}